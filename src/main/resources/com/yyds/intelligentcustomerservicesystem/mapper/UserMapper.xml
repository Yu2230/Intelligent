<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yyds.intelligentcustomerservicesystem.mapper.UserMapper">
   <insert id="insert" useGeneratedKeys="true" keyProperty="user_id">
       insert into user (user_id,user_name,email,password,sex,avatar,create_time)
       values (#{userId},#{userName},#{email},#{password},#{sex},#{avatar},#{createTime})
   </insert>

</mapper>
