<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yyds.intelligentcustomerservicesystem.mapper.SalaryMapper">

    <insert id="insertSalary" useGeneratedKeys="true" keyProperty="salary_id">
        insert into Salary (salary_id,user_id,salary,sned_name,create_time)
        values (#{salaryId},#{userId},#{salary},#{sendName},#{createTime})
    </insert>
</mapper>
